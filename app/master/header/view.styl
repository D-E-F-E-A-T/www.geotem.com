.header-master .header-menu .menu ul li:hover .submenu 
		display		:	block
		opacity		:	1
		visibility	:	visible

.submenu li a:hover
	color	:	red !important
	
.section
	col(1/6)
	text-align 	: center
	display		: inline
	
